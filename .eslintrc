{
  "extends": [
    "next/core-web-vitals",
    "prettier",
    "plugin:@next/next/recommended",
    "eslint:recommended"
  ],
  "plugins": ["prettier"],
  "rules": {
    "@next/next/no-img-element": "off",
    "prettier/prettier": "error",
    "jsx-a11y/alt-text": [0]
  },
  "ignorePatterns": ["public"],
  "settings": {
    "next": {
      "rootDir": "./src/"
    }
  }
}
